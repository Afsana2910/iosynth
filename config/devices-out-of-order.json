            {"type":"StringCycle", "name":"command", "values":["Alfa","Bravo","Charlie","Delta","Echo","Foxtrot"]},
            {"type":"IntRandom",   "name":"state", "min":0, "max":5, "format":"{\"ff\":%03d}"},
            {"type":"IntCycle",    "name":"level", "values": [1,2,8,9,11,2,3,4]},
            {"type":"StringCycle", "name":"switch", "values":["on", "off"]}
        ]
    }
]