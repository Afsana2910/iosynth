[
    {
        "type":"Simple",
        "uuid":{"type":"String", "value":"zzz"},
        "sampling":{"type":"Uniform", "min":3000, "max":5000},
        "copy":1,
        "out_of_order":0.01,
        "message_loss":0.01,
        "sensors":[
            {"type":"Timestamp",   "format":"yyyy-MM-dd'T'HH:mm:ss.SSSZ"},
